{
  "schema": {
    "name": {
      "type": "string",
      "required": true,
      "label": "Blog name"
    },
    "version": {
      "type": "string",
      "required": true,
      "label": "Blog version",
      "default": "0.1.0"
    },
    "description": {
      "type": "string",
      "required": false,
      "label": "Blog description",
      "default": "A backend-free blog system built on top of Vue.js"
    },
    "author": {
      "type": "string",
      "label": "Author"
    },
    "private": {
      "type": "boolean",
      "default": true
    }
  }
}
